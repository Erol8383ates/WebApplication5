@page
@model WebApplication5.Pages.Admin.Computers.DeleteModel

@{
    ViewData["Title"] = "Delete Computer";
}

<h2>Are you sure you want to delete this Computer?</h2>

@if (Model.Computer != null)
{
    <div>
        <p>Lokaal: @Model.Computer.Lokaal</p>
        <p>Naam: @Model.Computer.Naam</p>
       
       
    </div>

    <form method="post">
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-page="/Admin/Computers/Index" class="btn btn-secondary">Cancel</a>
    </form>
}

