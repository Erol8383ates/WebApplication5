@page
@model WebApplication5.Pages.Admin.Printers.DeleteModel

@{
    ViewData["Title"] = "Delete Printer";
}

<h2>Are you sure you want to delete this printer?</h2>

@if (Model.Printer != null)
{
    <div>
        <p>Naam: @Model.Printer.Naam</p>
        <p>Lokaal: @Model.Printer.Lokaal</p>
        <p>IP Address: @Model.Printer.Ipaddress</p>
        <p>Model: @Model.Printer.Model</p>
        <p>Created At: @Model.Printer.CreatedAt</p>
    </div>

    <form method="post">
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-page="/Admin/Printers/Index" class="btn btn-secondary">Cancel</a>
    </form>
}

